//selects the min element in each subarray of the loop and then swaps it with the first element

#include <bits/stdc++.h> 
void selectionSort(vector<int>& arr, int n)
{   
    // Write your code here.
    for(int i=0;i<n-1;i++){
        
        for(int j=i+1;j<n;j++){
               if(arr[j]<arr[i]){
                   swap(arr[i],arr[j]);
               }
        }
        
    }
}
