// brute force two loops finding sum
//optimal approach using hashtable check if target-nums[i] is present in the hash if then return ans then insert val and index of element into the hash

vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> ans;
        map<int,int>mp;
        for(int i=0;i<nums.size();i++){
            int a=target-nums[i];
            auto it=mp.find(a);
            if(it!=mp.end()){
                ans.push_back(i);
                ans.push_back(it->second);
                return ans;
            }
            mp.insert({nums[i],i});

        }
        return ans;
    }
