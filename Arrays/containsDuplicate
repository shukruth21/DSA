// brute force sort array and check adjacent elements
//optimal using hashmap
bool containsDuplicate(vector<int>& nums) {
        map<int,int>mp;
        for(int i=0;i<nums.size();i++){
            mp[nums[i]]++;
        }
        for(auto it: mp){
            if(it.second >1){
                return true;
            }
        }
        return false;
    }
