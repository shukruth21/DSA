vector<int> getSecondOrderElements(int n, vector<int> a) {
    // Write your code here.
    int l=INT_MIN,l2=INT_MIN,m=INT_MAX,m2=INT_MAX;
    for(int i=0;i<n;i++){
        if(a[i]<m){
            m2=m;
            m=a[i];
        }
        else if (a[i]<m2 && a[i]!=m){
            m2=a[i];
        }
        if(a[i]>l){
            l2=l;
            l=a[i];
        }
        else if (a[i]>l2 && a[i]!=l){
            l2=a[i];
        }
    }
    vector<int>ans={l2,m2};
    return ans;
}
