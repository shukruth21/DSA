//brute force sort the 2 strings and comparee
//optimal appraoch hashmap

bool isAnagram(string s, string t) {
        map<char,int>mp;
        if(s.length()!=t.length()){
            return false;
        }
        for(int i=0;i<s.length();i++){
            mp[s[i]]++;
        }
        for(int j=0;j<t.length();j++){
            mp[t[j]]--;
        }
        for(auto &it: mp){
            if(it.second >0){
                return false;
            }
        }
        return true;
    }
